<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#D81B60</color>
    <color name="colorBackground">#bbbbbb</color>
    <color name="colorPrimary">#008577</color>
    <color name="colorPrimaryDark">#00574B</color>
    <string name="app_name">TestLibrary</string>
    <style name="ActivityTitelText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:layout_marginStart">8dp</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="DialogBoxStyleTitle" parent="TextAppearance.AppCompat">
        <item name="android:textSize">20dp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="DialogBoxTextBody" parent="TextAppearance.AppCompat">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:textSize">15dp</item>
    </style>
    <style name="TableRowsTextBody" parent="TextAppearance.AppCompat">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:textSize">10dp</item>
    </style>
    <style name="TableRowsTitel" parent="TextAppearance.AppCompat">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:textSize">15dp</item>
    </style>
</resources>